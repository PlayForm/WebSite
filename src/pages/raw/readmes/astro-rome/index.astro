---
import PackagesRow from "@components/PackagesRow.astro";
import type { packageCell } from "../../../../options/packageCell";

const packages = new Map<string, Set<packageCell>>();

packages.set(
	"astro-rome",
	new Set<packageCell>(["npm:astro-compress", "npm:astro-critters"])
);
---

<>
	<table>
		<tr>
			<td colspan="4">
				<h2>
					<picture
						><source
							media="(prefers-color-scheme: dark)"
							srcset="https://raw.githubusercontent.com/image-repository/logos/main/.github/img/logomark-dark.svg"
						/><source
							media="(prefers-color-scheme: light)"
							srcset="https://raw.githubusercontent.com/image-repository/logos/main/.github/img/logomark-light.svg"
						/><img
							width="25"
							alt="Astro"
							src="https://raw.githubusercontent.com/image-repository/logos/main/.github/img/logomark-light.svg"
						/>
					</picture> Related
				</h2>
			</td>
		</tr>
		<PackagesRow packages={packages.get("astro-rome")} />
	</table>
</>
